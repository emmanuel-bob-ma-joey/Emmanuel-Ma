{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.{html,css,js,ico,png,jpg,jpeg,webp,wav}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/strava-api",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
